# standard functions
export(somgrid,batchsom,colorCode,mapToUnit,grid2lines,componentPlane,hitMap)
export(umatrix,error.kaskilagus,error.quantisation,som.tune,som.tunecontrol)
export(prototype.distances,distance.grid,sominit)
# somgrid S3 methods
S3method(print,somgrid)
S3method(summary,somgrid)
S3method(plot,somgrid)
# som S3 methods (generic)
S3method(print,som)
S3method(summary,som)
# somnum S3 methods
S3method(plot,somnum)
S3method(predict,somnum)
# default S3 methods
S3method(sominit,default)
S3method(batchsom,default)
# relational S3 methods (based on the dist class)
S3method(sominit,dist)
S3method(batchsom,dist)
# sompdist S3 methods
S3method(plot,sompdist)
S3method(as.matrix,sompdist)
S3method(as.dist,sompdist)
